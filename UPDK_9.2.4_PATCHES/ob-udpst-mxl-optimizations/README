# go to prplos directory
cd prplos

# clone updk_patches repository from GitHub.
git clone https://github.com/maxlinear/updk_patches.git

# create patches directory under the obudpst directory
mkdir feeds/feed_prplos/net_services/apps/obudpst/patches

# copy all the patches present in the ob-udpst-mxl-optimizations directory
cp updk_patches/UPDK_9.2.4_PATCHES/ob-udpst-mxl-optimizations/* feeds/feed_prplos/net_services/apps/obudpst/patches/
